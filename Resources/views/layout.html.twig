<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Email Handler{% endblock title %}</title>
        {% block stylesheets %}
            <!--[if IE]>
                    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/bvitemplate/dist/css/all-ie-only.css') }}" />
            <![endif]-->
			
            <!-- Bootstrap 3.3.4 -->
            <link href="{{ asset('bundles/bvitemplate/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
            <!-- Font Awesome Icons -->
            <link href="{{ asset('bundles/bvitemplate/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css" />
            <!-- Ionicons -->
            <link href="{{ asset('bundles/bvitemplate/css/ionicons.min.css') }}" rel="stylesheet" type="text/css" />
            <!-- Theme style -->

            <!-- slick-->
            <link href="{{ asset('bundles/bvitemplate/css/slick.css') }}" rel="stylesheet">

            <link href="{{ asset('bundles/bvitemplate/dist/css/adminLTE.min.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('bundles/bvitemplate/dist/css/admin.css') }}" rel="stylesheet" type="text/css" />
            <!-- AdminLTE Skins. We have chosen the skin-blue for this starter
                  page. However, you can choose any other skin. Make sure you
                  apply the skin class to the body tag so the changes take effect.
            -->
            <link href="{{ asset('bundles/bvitemplate/dist/css/skins/skin-green.min.css') }}" rel="stylesheet" type="text/css" />
            <!-- iCheck -->
            <link href="{{ asset('bundles/bvitemplate/plugins/iCheck/square/green.css') }}" rel="stylesheet" type="text/css" />

            <link href="{{ asset('bundles/bvitemplate/dist/css/responsive.css') }}" rel="stylesheet" type="text/css" />
        {% endblock stylesheets %}

        {% block javascripts %}

            <!-- REQUIRED JS SCRIPTS -->

            <!-- jQuery 2.1.1 -->
            <script src="{{ asset('bundles/bvitemplate/js/jquery-2.1.1.min.js') }}"></script>
            <!-- Bootstrap 3.3.2 JS -->
            <script src="{{ asset('bundles/bvitemplate/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
            <!-- AdminLTE App -->
            <script src="{{ asset('bundles/bvitemplate/dist/js/app.min.js') }}" type="text/javascript"></script>
            <!-- iCheck -->
            <script src="{{ asset('bundles/bvitemplate/plugins/iCheck/icheck.min.js') }}" type="text/javascript"></script>

            <!--Slick js-->
            <script src="{{ asset('bundles/bvitemplate/js/slick.min.js') }}"></script>
            
            <!-- End Js -->
            <script type="text/javascript" src="{{ asset('bundles/bvitemplate/js/jquery.validation.min.js') }}"></script>
            

            <script type="text/javascript" src="{{ asset('bundles/bvitemplate/js/custom-validation.js') }}"></script>
            
            <script src="{{ asset('bundles/bvitemplate/js/jquery-ui.js') }}"></script>
             <script src="{{ asset('bundles/bvitemplate/js/jquery.fileupload.min.js') }}"></script>
            <!-- Optionally, you can add Slimscroll and FastClick plugins.
                  Both of these plugins are recommended to enhance the
                  user experience. Slimscroll is required when using the
                  fixed layout. -->
        {% endblock javascripts %}
    </head>
    <body>
        <h1 class="text-center">Welcome To BVI E-Mail Template Management</h1>
        <hr>
        <div class="container">
            {% block body %}{% endblock %}
        </div>
        <script type="text/javascript">
            $(function () {
                $(".dataTable").wrap("<div class='responsive_table'></div>");
            });
        </script>
    </body>
</html>